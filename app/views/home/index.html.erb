<h1>List of Posts!</h1>
<br><br>
    <%@posts.each do |x| %>
        Title: <%=x.title%> <br><br>
        Content: <%=x.content%><br>
        <a href='/home/destroy/<%=x.id%>'>[DELETE]</a><br>
        <a href='/home/edit/<%=x.id%>'>[EDIT]</a><br>
    <%end%>
    
    
    <a href ="/home/new">Write a new post</a>